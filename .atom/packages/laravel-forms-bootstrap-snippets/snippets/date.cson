'.text.html.php.blade':
  'date':
    'prefix': 'bootdate'
    'body': """
        <div class="form-group{{ \$errors->has('${1:date}') ? ' has-error' : '' }}">
            {!! Form::label(\'${1:date}\', \'${2:Date}\') !!}
            {!! Form::date(\'${1:date}\',${3: null}, [\'class\' => \'form-control\'${4:, \'required\' => \'required\'}]) !!}
            <small class="text-danger">{{ \$errors->first(\'${1:date}\') }}</small>
        </div>
    """
